<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BlogMapper">
		<select id="myBatisTest" resultType="java.util.Map" parameterType="java.util.Map">	<!-- resultType = 반환타입  parameterType = 인입타입-->
		 select * from emp where empno = #{empno}
	</select>
		<select id="myBooks" resultType="java.util.Map" parameterType="java.util.Map">
		 select TO_CHAR(sysdate,'YYYY-MM-DD') as "curday" from dual
	</select>
</mapper>
