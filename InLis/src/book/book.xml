<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BlogMapper">
	<insert id="bookinsert" parameterType="java.util.Map">
		<![CDATA[INSERT INTO BOOKLIST(BOOKNO,BOOKNM,IMG,BOOKTS) VALUES (SEQ_JDBC_BOARD_NO.nextval,#{ti},'img'||SEQ_JDBC_BOARD_NO.currval||'.png',#{mo})]]>
	</insert>
	<select id="booklist" parameterType="Map" resultType="java.util.Map">
	<![CDATA[select BOOKNO,BOOKNM,BOOKTS from BOOKLIST where BOOKNM LIKE #{key}||'%']]>
	</select>
</mapper>
