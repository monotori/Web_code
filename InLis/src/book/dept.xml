<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BlogMapper">
	<insert id="deptinsert" parameterType="java.util.Map">
		<![CDATA[INSERT INTO dept(deptno,dname,loc) VALUES(#{deptno},#{dname},#{loc})]]>
	</insert>
	<update id="deptupdate" parameterType="java.util.Map">
		<![CDATA[update dept set dname = #{dname} where deptno = #{deptno}]]>
	</update>
	<select id="myBatisTest" resultType="java.util.Map"
		parameterType="java.util.Map">	<!-- resultType = 반환타입 parameterType = 인입타입 -->
		<![CDATA[select deptno,dname,loc from dept]]>
	</select>

	<delete id="insertAuthor" parameterType="list">
		<![CDATA[DELETE from dept where deptno IN]]>
		<foreach item="item" index="index" collection="list"
			separator="," open="(" close=")">
			#{item}
		</foreach>
	</delete>
	<select id="proc_emp_sal_update" resultSets="Map" statementType="CALLABLE">
		{call proc_emp_sal_update
		(
		#{deptno,jdbcType=NUMERIC,javaType=int,mode=IN},
		#{msg,jdbcType=VARCHAR,javaType=String,mode=OUT}
		)} 
	</select>
</mapper>
